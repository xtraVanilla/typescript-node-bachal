{
  "compilerOptions": {
    "lib": [
      "es2018",
      "dom",
      "esnext"
    ],
    "target": "es2018",
    "module": "commonjs",
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "inlineSourceMap": false,
  },
}
